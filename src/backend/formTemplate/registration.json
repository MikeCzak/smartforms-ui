{
  "formTitle": "User Registration & Preferences",
  "sections": [
    {
      "title": "Personal Information",
      "info": "Please provide personal information here",
      "fields": [
        {
          "name": "first_name",
          "type": "text",
          "label": "First Name",
          "required": true,
          "minLength": 2,
          "maxLength": 50
        },
        {
          "name": "last_name",
          "type": "text",
          "label": "Last Name",
          "required": true,
          "minLength": 2,
          "maxLength": 50
        },
        {
          "name": "dob",
          "type": "date",
          "label": "Date of Birth",
          "required": true
        },
        {
          "name": "gender",
          "type": "choice",
          "label": "Gender",
          "options": ["Male", "Female", "Other"],
          "required": true,
          "choiceType": "single"
        },
        {
          "name": "phone",
          "type": "tel",
          "label": "Phone Number",
          "pattern": "^\\+?[1-9]\\d{1,14}$",
          "required": true
        },
        {
          "name": "email",
          "type": "email",
          "label": "Email",
          "required": true
        }
      ]
    },
    {
      "title": "Address Details",
      "fields": [
        {
          "name": "street",
          "type": "text",
          "label": "Street Address",
          "required": true
        },
        {
          "name": "city",
          "type": "text",
          "label": "City",
          "required": true
        },
        {
          "name": "country",
          "type": "choice",
          "label": "Country",
          "options": ["United States", "Canada", "Afghanistan", "Albania", "Algeria", "American Samoa", "Andorra", "Angola", "Anguilla", "Antarctica", "Antigua and/or Barbuda", "Argentina", "Armenia", "Aruba", "Australia", "Austria", "Azerbaijan", "Bahamas", "Bahrain", "Bangladesh", "Barbados", "Belarus", "Belgium", "Belize", "Benin", "Bermuda", "Bhutan", "Bolivia", "Bosnia and Herzegovina", "Botswana", "Bouvet Island", "Brazil", "British Indian Ocean Territory", "Brunei Darussalam", "Bulgaria", "Burkina Faso", "Burundi", "Cambodia", "Cameroon", "Cape Verde", "Cayman Islands", "Central African Republic", "Chad", "Chile", "China", "Christmas Island", "Cocos (Keeling) Islands", "Colombia", "Comoros", "Congo", "Cook Islands", "Costa Rica", "Croatia (Hrvatska)", "Cuba", "Cyprus", "Czech Republic", "Denmark", "Djibouti", "Dominica", "Dominican Republic", "East Timor", "Ecudaor", "Egypt", "El Salvador", "Equatorial Guinea", "Eritrea", "Estonia", "Ethiopia", "Falkland Islands (Malvinas)", "Faroe Islands", "Fiji", "Finland", "France", "France, Metropolitan", "French Guiana", "French Polynesia", "French Southern Territories", "Gabon", "Gambia", "Georgia", "Germany", "Ghana", "Gibraltar", "Greece", "Greenland", "Grenada", "Guadeloupe", "Guam", "Guatemala", "Guinea", "Guinea-Bissau", "Guyana", "Haiti", "Heard and Mc Donald Islands", "Honduras", "Hong Kong", "Hungary", "Iceland", "India", "Indonesia", "Iran (Islamic Republic of)", "Iraq", "Ireland", "Israel", "Italy", "Ivory Coast", "Jamaica", "Japan", "Jordan", "Kazakhstan", "Kenya", "Kiribati", "Korea, Democratic People's Republic of", "Korea, Republic of", "Kosovo", "Kuwait", "Kyrgyzstan", "Lao People's Democratic Republic", "Latvia", "Lebanon", "Lesotho", "Liberia", "Libyan Arab Jamahiriya", "Liechtenstein", "Lithuania", "Luxembourg", "Macau", "Macedonia", "Madagascar", "Malawi", "Malaysia", "Maldives", "Mali", "Malta", "Marshall Islands", "Martinique", "Mauritania", "Mauritius", "Mayotte", "Mexico", "Micronesia, Federated States of", "Moldova, Republic of", "Monaco", "Mongolia", "Montserrat", "Morocco", "Mozambique", "Myanmar", "Namibia", "Nauru", "Nepal", "Netherlands", "Netherlands Antilles", "New Caledonia", "New Zealand", "Nicaragua", "Niger", "Nigeria", "Niue", "Norfork Island", "Northern Mariana Islands", "Norway", "Oman", "Pakistan", "Palau", "Panama", "Papua New Guinea", "Paraguay", "Peru", "Philippines", "Pitcairn", "Poland", "Portugal", "Puerto Rico", "Qatar", "Reunion", "Romania", "Russian Federation", "Rwanda", "Saint Kitts and Nevis", "Saint Lucia", "Saint Vincent and the Grenadines", "Samoa", "San Marino", "Sao Tome and Principe", "Saudi Arabia", "Senegal", "Seychelles", "Sierra Leone", "Singapore", "Slovakia", "Slovenia", "Solomon Islands", "Somalia", "South Africa", "South Georgia South Sandwich Islands", "South Sudan", "Spain", "Sri Lanka", "St. Helena", "St. Pierre and Miquelon", "Sudan", "Suriname", "Svalbarn and Jan Mayen Islands", "Swaziland", "Sweden", "Switzerland", "Syrian Arab Republic", "Taiwan", "Tajikistan", "Tanzania, United Republic of", "Thailand", "Togo", "Tokelau", "Tonga", "Trinidad and Tobago", "Tunisia", "Turkey", "Turkmenistan", "Turks and Caicos Islands", "Tuvalu", "Uganda", "Ukraine", "United Arab Emirates", "United Kingdom", "United States minor outlying islands", "Uruguay", "Uzbekistan", "Vanuatu", "Vatican City State", "Venezuela", "Vietnam", "Virigan Islands (British)", "Virgin Islands (U.S.)", "Wallis and Futuna Islands", "Western Sahara", "Yemen", "Yugoslavia", "Zaire", "Zambia", "Zimbabwe"],
          "required": true,
          "choiceType": "single"
        },
        {
          "name": "zipcode",
          "type": "text",
          "label": "ZIP Code",
          "required": true,
          "patternByCountry": {
            "USA": "^[0-9]{5}(-[0-9]{4})?$",
            "Canada": "^[A-Za-z]\\d[A-Za-z] ?\\d[A-Za-z]\\d$",
            "Germany": "^[0-9]{5}$",
            "UK": "^[A-Z]{1,2}[0-9R][0-9A-Z]?[ ]?[0-9][A-Z]{2}$",
            "France": "^[0-9]{5}$"
          }
        }
      ]
    },
    {
      "title": "Account Information",
      "fields": [
        {
          "name": "username",
          "type": "text",
          "label": "Username",
          "required": true,
          "minLength": 5,
          "maxLength": 20
        },
        {
          "name": "password",
          "type": "password",
          "label": "Password",
          "required": true,
          "minLength": 8,
          "maxLength": 100,
          "pattern": "^(?=.*[A-Z])(?=.*[a-z])(?=.*\\d)(?=.*[!@#$%^&*])[A-Za-z\\d!@#$%^&*]{8,}$"
        }
      ]
    },
    {
      "title": "Preferences",
      "fields": [
        {
          "name": "newsletter",
          "type": "choice",
          "label": "Subscribe to newsletter",
          "options": ["Yes", "No"],
          "required": true,
          "choiceType": "single"
        },
        {
          "name": "topics",
          "type": "choice",
          "label": "Interested Topics",
          "options": ["Technology", "Sports", "Music", "Movies", "Science"],
          "required": false,
          "choiceType": "multiple"
        }
      ]
    },
    {
      "title": "Payment Information",
      "fields": [
        {
          "name": "payment_method",
          "type": "choice",
          "label": "Payment Method",
          "options": ["Credit Card", "PayPal", "Bank Transfer"],
          "required": true,
          "choiceType": "single"
        },
        {
          "name": "iban",
          "type": "text",
          "label": "IBAN",
          "pattern": "^[A-Z]{2}\\d{2}[A-Z0-9]{1,30}$",
          "required": true,
          "dependsOn": {
            "field": "payment_method",
            "value": "Bank Transfer"
          }
        },
        {
          "name": "credit_card_number",
          "type": "text",
          "label": "Credit Card Number",
          "pattern": "^[0-9]{16}$",
          "required": true,
          "dependsOn": {
            "field": "payment_method",
            "value": "Credit Card"
          }
        }
      ]
    },
    {
      "title": "Emergency Contact (Nested Subform)",
      "fields": [
        {
          "name": "emergency_contact",
          "type": "form",
          "label": "Emergency Contact Information",
          "subform": {
            "fields": [
              {
                "name": "contact_name",
                "type": "text",
                "label": "Contact Name",
                "required": true
              },
              {
                "name": "contact_phone",
                "type": "tel",
                "label": "Contact Phone",
                "required": true,
                "pattern": "^\\+?[1-9]\\d{1,14}$"
              },
              {
                "name": "contact_relation",
                "type": "text",
                "label": "Relation",
                "required": true
              }
            ]
          }
        }
      ]
    },
    {
      "title": "Employment History (Repeatable Subform)",
      "fields": [
        {
          "name": "previous_employer",
          "type": "form",
          "label": "Previous Employer Information",
          "repeatable": true,
          "subform": {
            "fields": [
              {
                "name": "company_name",
                "type": "text",
                "label": "Company Name",
                "required": true
              },
              {
                "name": "job_title",
                "type": "text",
                "label": "Job Title",
                "required": true
              },
              {
                "name": "employment_years",
                "type": "number",
                "label": "Years Worked",
                "min": 0,
                "max": 50,
                "required": true
              }
            ]
          }
        }
      ]
    },
    {
      "title": "Additional Information",
      "fields": [
        {
          "name": "ssn",
          "type": "text",
          "label": "Social Security Number",
          "pattern": "^(?!000|666)[0-9]{3}-(?!00)[0-9]{2}-(?!0000)[0-9]{4}$",
          "required": true
        },
        {
          "name": "is_student",
          "type": "choice",
          "label": "Are you a student?",
          "options": ["Yes", "No"],
          "required": true,
          "choiceType": "single"
        },
        {
          "name": "university",
          "type": "text",
          "label": "University Name",
          "required": true,
          "dependsOn": {
            "field": "is_student",
            "value": "Yes"
          }
        },
        {
          "name": "graduation_year",
          "type": "number",
          "label": "Expected Graduation Year",
          "min": 2024,
          "max": 2035,
          "required": true,
          "dependsOn": {
            "field": "is_student",
            "value": "Yes"
          }
        }
      ]
    },
    {
      "title": "Terms & Conditions",
      "fields": [
        {
          "name": "agree_terms",
          "type": "choice",
          "label": "I agree to the Terms and Conditions",
          "options": ["Yes"],
          "required": true,
          "choiceType": "single"
        }
      ]
    }
  ],
  "pagination": {
    "enabled": true,
    "fieldsPerPage": 5
  }
}
